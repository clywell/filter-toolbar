{
  "name": "@clywell/filter-toolbar",
  "version": "0.0.2",
  "description": "A flexible and powerful React filter system with framework adapters for Next.js, React Router, and more",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/styles.css",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "echo 'No tests specified' && exit 0",
    "test:watch": "echo 'No tests specified' && exit 0",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "filter",
    "toolbar",
    "components",
    "nextjs",
    "react-router",
    "typescript",
    "ui"
  ],
  "author": "Clywell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clywell/filter-toolbar.git"
  },
  "bugs": {
    "url": "https://github.com/clywell/filter-toolbar/issues"
  },
  "homepage": "https://github.com/clywell/filter-toolbar#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.542.0"
  },
